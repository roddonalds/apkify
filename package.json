{
  "name": "com.ropsoft.ticket4fun",
  "displayName": "ticket4.fun",
  "version": "1.1.62",
  "description": "Versão móvel do systema de atendimento online ticket4.fun",
  "main": "index.js",
  "scripts": {
    "edit:rc": "nano /root/.bashrc",
    "set:icon": "cordova-icon",
    "git:start": "bash scripts/git/start.sh",
    "upload:cdn": "node scripts/uploadCdn.js",
    "both:build": "cordova build android && cordova build android --release",
    "debug:build": "cordova build android",
    "release:build": "cordova build android --release",
    "platform:reset": "cordova platform remove android && cordova platform add android@9.1.0"
  },
  "keywords": [ "ecosystem:cordova" ],
  "author": "ropsoft",
  "license": "Apache-2.0",
  "devDependencies": {
    "all": "^0.0.0",
    "cordova-android": "^9.1.0",
    "cordova-plugin-app-version": "^0.1.14",
    "cordova-plugin-whitelist": "^1.3.4",
    "json-beautify": "^1.1.1",
    "xml2js": "^0.6.2"
  },
  "cordova": {
    "plugins": { "cordova-plugin-whitelist": {}, "cordova-plugin-app-version": {} },
    "platforms": [ "android" ]
  },
  "dependencies": { "cordova-icon": "^1.0.0", "shelljs": "^0.8.5" }
}