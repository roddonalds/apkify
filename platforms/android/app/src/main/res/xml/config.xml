<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ropsoft.ticket4fun" version="1.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AppVersion">
        <param name="android-package" value="uk.co.whiteoctober.cordova.AppVersion" />
    </feature>
    <name>ticket4.fun</name>
    <description>
        Versão móvel do systema de atendimento online ticket4.fun
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        ropsoft
    </author>
    <hook src="scripts/afterBuild.js" type="after_build" />
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-exported" value="true" />
    <preference name="jdkVersion" value="8" />
    <preference name="android-targetSdkVersion" value="33" />
    <preference name="android-compileSdkVersion" value="33" />
    <preference name="KeepRunning" value="true" />
    <preference name="Fullscreen" value="true" />
</widget>
